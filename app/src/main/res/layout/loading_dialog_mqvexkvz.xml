<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/theme_1_yellow_ofr"
    android:padding="20dp">

    <TextView
        android:id="@+id/tv_loading_dialog_mqvexkvz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dela_gothic_mqvexkvz"
        android:text="@string/loading_mqvexkvz"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar_mqvexkvz"
        style="?android:attr/progressBarStyle"
        android:layout_width="34dp"
        android:layout_height="30dp"
        android:layout_margin="20dp"
        android:indeterminateTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_loading_dialog_mqvexkvz" />
</androidx.constraintlayout.widget.ConstraintLayout>